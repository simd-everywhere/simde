[binaries]
c = ['riscv64-unknown-linux-gnu-gcc', '-march=rv64gcv', '-mrvv-vector-bits=128']
cpp = ['riscv64-unknown-linux-gnu-g++', '-march=rv64gcv']
ar = 'riscv64-unknown-linux-gnu-ar'
strip = 'riscv64-unknown-linux-gnu-strip'

[host_machine]
system = 'linux'
cpu_family = 'riscv64'
cpu = 'riscv64'
endian = 'little'